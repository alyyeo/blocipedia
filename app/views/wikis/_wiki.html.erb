<div class="row">
    <% wikis.each do |wiki| %>
        <div class="col s4">
            <div class="card blue-grey darken-4">
                <div class="card-action">
                    <%= link_to wiki.title, wiki, class: 'card-title' %>
                </div>
            </div>
        </div>
    <% end %>
</div>